$(".affiliation").on('ajax:success','#new_affiliation_<%= @team.id %>', function() {
    var tr = $(this).closest('tr')
    tr.fadeOut();
    tr.remove();
    var newRow = "<%= j render 'leagues/row_edit_team_table', team: @team, action: 'remove' %>";
    $('#remove-table .teams-league').append(newRow);
});
